<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ed.shuneladmin.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
        <action
            android:id="@+id/action_homeFragment_to_userFragment"
            app:destination="@id/userFragment" />
    </fragment>
    <fragment
        android:id="@+id/userFragment"
        android:name="com.ed.shuneladmin.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" >
        <action
            android:id="@+id/action_userFragment_to_userdetailFragment"
            app:destination="@id/userdetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/member_newsFragment"
        android:name="com.ed.shuneladmin.Member_newsFragment"
        android:label="fragment_member_news"
        tools:layout="@layout/fragment_member_news" />
    <fragment

        android:id="@+id/adminFragment"
        android:name="com.ed.shuneladmin.AdminFragment"
        android:label="fragment_admin"
        tools:layout="@layout/fragment_admin" >
        <action
            android:id="@+id/action_adminFragment_to_adminNewFragment"
            app:destination="@id/adminNewFragment" />
    </fragment>

    <fragment
        android:id="@+id/ordersManagementFragment"
        android:name="com.ed.shuneladmin.OrdersManagementFragment"
        android:label="fragment_orders_management"
        tools:layout="@layout/fragment_orders_management" />
    <fragment
        android:id="@+id/userdetailFragment"
        android:name="com.ed.shuneladmin.UserdetailFragment"
        android:label="fragment_userdetail"
        tools:layout="@layout/fragment_userdetail" />
    <fragment
        android:id="@+id/adminNewFragment"
        android:name="com.ed.shuneladmin.AdminNewFragment"
        android:label="fragment_admin_new"
        tools:layout="@layout/fragment_admin_new" >
        <action
            android:id="@+id/action_adminNewFragment_to_adminNewDetailFragment"
            app:destination="@id/adminNewDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/adminNewDetailFragment"
        android:name="com.ed.shuneladmin.AdminNewDetailFragment"
        android:label="fragment_admin_new_detail"
        tools:layout="@layout/fragment_admin_new_detail" />

</navigation>